<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.kalpa.batch.multi.sbbatmultidb.repository.des.DesWordsRepository">

	<insert id="insert" parameterType="DesWords">
		INSERT INTO words
			(han,eng,china)
		VALUES
		    (#{han}, #{eng}, #{china})
	</insert>

</mapper>